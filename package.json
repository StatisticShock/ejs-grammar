{
	"name": "ejs-language-support-with-delimiters",
	"displayName": "EJS language support with delimiters",
	"description": "2019 - EJS language support for Visual Studio Code.",
	"version": "1.3.2",
	"license": "MIT",
	"icon": "assets/icon.png",
	"publisher": "StatisticShock",
	"author": {
		"name": "Derek Thomas"
	},
	"engines": {
		"vscode": "^0.10.5"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"EJS",
		"embedded javascript",
		"syntax highlighting"
	],
	"contributes": {
		"languages": [
			{
				"id": "ejs",
				"aliases": [
					"EJS",
					"ejs"
				],
				"extensions": [
					".ejs"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"scopeName": "text.html.ejs",
				"path": "./syntaxes/ejs.json",
				"injectTo": [
					"text.html"
				],
				"embeddedLanguages": {
					"meta.embedded.block.ejs": "ejs",
					"meta.embedded.block.javascript": "source.js"
				}
			}
		],
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/snippets.json"
			}
		],
		"configurationDefaults": {
			"files.associations": {
				"*.ejs": "html"
			},
			"emmet.includeLanguages": {
				"ejs": "html"
			}
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/StatisticShock/ejs-grammar.git"
	}
}